local player = game.Players:FindFirstChild("missedtonight")

function genstop(stompname)
local g =  player:FindFirstChild("DataFolder"):FindFirstChild("InventoryData_"):FindFirstChild("Stomp Effects").Car:Clone()

g.Name = stompname
g.Parent = player:FindFirstChild("DataFolder"):FindFirstChild("InventoryData_"):FindFirstChild("Stomp Effects") 

end

genstop("Seraph")
