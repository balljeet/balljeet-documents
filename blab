local player = game.Players:FindFirstChild("UsernameMaster_2")

function genstop(stompname)
local g =  player:FindFirstChild("DataFolder"):FindFirstChild("InventoryData_"):FindFirstChild("Stomp Effects").Bus:Clone()

g.Name = stompname
g.Parent = player:FindFirstChild("DataFolder"):FindFirstChild("InventoryData_"):FindFirstChild("Stomp Effects") 

end

genstop("Seraph")
genstop("Carti")
genstop("CresentMoon")
genstop("WhiteNova")
